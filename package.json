        "title": "Open Component in Browser",
        "category": "OpenShift"
      },
      {
        "command": "openshift.storage.create",
        "title": "New Storage",
        "category": "OpenShift"
      },
      {
        "command": "openshift.storage.delete.palette",
        "title": "Delete Storage",
        "category": "OpenShift"
      },
      {
        "command": "openshift.storage.delete",
        "title": "Delete",
